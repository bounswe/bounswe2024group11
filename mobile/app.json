{
  "expo": {
    "name": "Zenith",
    "slug": "Zenith",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zenith-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/zenith-logo.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/zenith-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tenacke.Zenith"
    },
    "web": {
      "favicon": "./assets/zenith-logo.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "52befeee-3d71-491e-afe4-66c31f2c710c"
      }
    }
  }
}
